{
    "title": "ConvertVideos",
    "authors": [ "Mike Morano <mmorano@mikeandwan.us>" ],
    "description": "A small utility to convert videos for my website",
    "version": "0.1.0",
    "packOptions": {
        "licenseUrl": "https://github.com/AerisG222/ConvertVideos/blob/master/LICENSE.md",
        "projectUrl": "https://github.com/AerisG222/ConvertVideos",
        "summary": "A small utility to convert videos for my website",
        "tags": [ "video", "movie", "convert", "ffmpeg" ]
    },
    "buildOptions": {
        "debugType": "portable",
        "emitEntryPoint": true,
        "warningsAsErrors": true
    },
    "commands": {
        "run": "ConvertVideos" 
    },
    "dependencies": {
        "System.CommandLine": "0.1.0-e170103-1"
    },
    "frameworks": {
        "netcoreapp1.1": {
            "dependencies": {
                "Microsoft.NETCore.App": {
                    "version": "1.1.0",
                    "type": "platform"
                }
            },
            "imports": [
                "dotnet55",
                "portable-dnxcore50+net45+win8+wp8+wpa81"
            ]
        }
    }
}
