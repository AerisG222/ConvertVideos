<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>A small utility to convert videos for my website</Description>
    <AssemblyTitle>ConvertVideos</AssemblyTitle>
    <VersionPrefix>0.1.0</VersionPrefix>
    <Authors>Mike Morano &lt;mmorano@mikeandwan.us&gt;</Authors>
    <TargetFramework>netcoreapp1.1</TargetFramework>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <DebugType>portable</DebugType>
    <AssemblyName>ConvertVideos</AssemblyName>
    <OutputType>Exe</OutputType>
    <PackageId>ConvertVideos</PackageId>
    <PackageTags>video;movie;convert;ffmpeg</PackageTags>
    <PackageProjectUrl>https://github.com/AerisG222/ConvertVideos</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/AerisG222/ConvertVideos/blob/master/LICENSE.md</PackageLicenseUrl>
    <PackageTargetFallback>$(PackageTargetFallback);dotnet55;portable-dnxcore50+net45+win8+wp8+wpa81</PackageTargetFallback>
    <RuntimeFrameworkVersion>1.1.1</RuntimeFrameworkVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.CommandLine" Version="0.1.0-e170309-1" />
  </ItemGroup>

</Project>
